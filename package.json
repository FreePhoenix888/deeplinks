{
  "name": "@deep-foundation/deeplinks",
  "version": "0.0.9",
  "scripts": {
    "docker-start": "cd local && docker-compose up",
    "docker-stop": "cd local && docker-compose down",
    "build": "tsc --project ./tsconfig.json",
    "dev": "(cd local && docker-compose down || true) && PORT=3006 nodemon index.ts",
    "tsnode": "PORT=3006 ts-node index.ts",
    "package:build": "tsc --project tsconfig.json && (cd ./migrations && rimraf ./*.js && rimraf ./*.js.map && rimraf ./*.d.ts)",
    "package:unbuild": "rimraf ./*.js && rimraf ./*.js.map && rimraf ./*.d.ts && (cd ./migrations && rimraf ./*.js && rimraf ./*.js.map && rimraf ./*.d.ts && cd ../) && (cd ./imports && rimraf ./*.js && rimraf ./*.js.map && rimraf ./*.d.ts && cd ../) && (cd ./imports/gql && rimraf ./*.js && rimraf ./*.js.map && rimraf ./*.d.ts && cd ../)",
    "package:publish": "npm run package:build && npm publish --access public && npm run package:unbuild",
    "migrate": "npx migrate up --compiler=ts:./compiler/index.js",
    "unmigrate": "npx migrate down --compiler=ts:./compiler/index.js",
    "hasura-docker": "cross-env JWT_SECRET=\"{\\\"type\\\":\\\"HS256\\\",\\\"key\\\":\\\"3EK6FD+o0+c7tzBNVfjpMkNDi2yARAAKzQlk8O2IKoxQu4nF7EdAh8s3TwpHwrdWT6R\\\"}\" npm run hasura-docker-core",
    "hasura-docker-core": "cd ./node_modules/@deep-foundation/hasura/local && docker-compose up -d"
  },
  "dependencies": {
    "@apollo/client": "^3.4.9",
    "@deep-foundation/materialized-path": "0.0.6",
    "@deep-foundation/react-hasura": "^0.0.3",
    "@deep-foundation/store": "^0.0.3",
    "@types/react": "^17.0.19",
    "apollo-boost": "^0.4.9",
    "apollo-server": "^2.21.0",
    "apollo-server-express": "^3.5.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "fkill": "^8.0.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "internal-ip": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "uniqid": "^5.3.0",
    "wait-on": "^6.0.0"
  },
  "peerDependencies": {
    "@deep-foundation/hasura": "*",
    "react": "*"
  },
  "engines": {
    "node": "^14.15.0"
  },
  "devDependencies": {
    "@deep-foundation/hasura": "0.0.4",
    "@types/node": "^14.17.34",
    "typescript": "^4.4.3"
  }
}
